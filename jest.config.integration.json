{
    "roots": ["test/integration"],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
        "\\.[jt]sx?$": ["ts-jest", {
            "diagnostics": {
                "ignoreCodes": [7005, 7006, 7034]
            },
            "ts-config": "tsconfig.dev.json"
        }]
    },
    "clearMocks": true
}
